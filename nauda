summa = float(input("cik tev ir naudas?"))
summa2 = float(input("cik tu gribi?"))
procent = float(input("gada likme"))
count = 0

while summa < summa2:
    summa += summa*(procent/100)
    count +=1
    print(count, "gads", round (summa,2))


